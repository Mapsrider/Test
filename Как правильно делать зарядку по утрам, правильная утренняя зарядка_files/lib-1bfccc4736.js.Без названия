function catalogDropdownHoverInit(){var o,e,t,a=150;$(".js-catalog").hover(function(t){clearTimeout(o);var n=$(this);e&&e!=n&&e.hasClass("active")&&toggleActiveTarget(e),e=$(this),e.hasClass("active")||(o=setTimeout(function(){toggleActiveTarget(e)},a))},function(n){clearTimeout(o),e.hasClass("active")&&(o=setTimeout(function(){t||toggleActiveTarget(e)},a))}),$(".js-catalog-dropdown").hover(function(o){t=!0,console.log("open")},function(o){t=!1})}$(function(){window.loader=$("#loader");var o=["https://cartprotector.com/"];window.excludeUrlFromLoader=function(e){o.push(e)},$(document).ajaxSend(function(e,t,a){$.inArray(a.url,o)!==-1||"undefined"!=typeof a.data&&a.data.indexOf("hide_loader")+1||window.loader.hasClass("hidden")&&$(window.loader).removeClass("hidden"),a.url.indexOf("fit2u.")>0&&t.setRequestHeader("X-CSRF-TOKEN",$('meta[name="csrf-token"]').attr("content"))}),$(document).ajaxStart(function(){}),$(document).ajaxStop(function(){window.loader.hasClass("hidden")||$(window.loader).addClass("hidden")})}),catalogDropdownHoverInit();